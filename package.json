{
  "name": "it-traveler",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --open",
    "build-only": "bunx --bun vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "check": "bunx steiger ./src",
    "check:watch": "bunx steiger ./src --watch",
    "format": "prettier --write src/",
    "prepare": "lefthook install",
    "clean": "rimraf dist && rimraf node_modules"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.8",
    "@feature-sliced/filesystem": "^3.0.1",
    "@unhead/vue": "^2.0.8",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.503.0",
    "pinia": "^3.0.2",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@feature-sliced/steiger-plugin": "^0.5.5",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/vite": "^4.1.4",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/eslint-plugin": "1.1.43",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.25.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.11",
    "lint-staged": "^15.5.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
